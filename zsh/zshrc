# environments
source ~/.config/zsh/brew
source ~/.config/zsh/flyctl
source ~/.config/zsh/kitty
source ~/.config/zsh/nvm
source ~/.config/zsh/ocaml
source ~/.config/zsh/python
source ~/.config/zsh/rust
source ~/.config/zsh/starship
source ~/.config/zsh/zoxide

# config
source ~/.config/zsh/aliases
source ~/.config/zsh/colours
source ~/.config/zsh/functions
source ~/.config/zsh/hooks
source ~/.config/zsh/keys
source ~/.config/zsh/opts


export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
